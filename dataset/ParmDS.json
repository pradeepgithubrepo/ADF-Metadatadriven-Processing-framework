{
	"name": "SourceDS",
	"properties": {
		"linkedServiceName": {
			"referenceName": "BlobLinkedservice",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "@pipeline().parameters.SourceFile"
			},
			"FilePath": {
				"type": "string",
				"defaultValue": "@pipeline().parameters.SourcePath"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": "@dataset().FileName",
				"fileSystem": {
					"value": "@dataset().FilePath",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "movie",
				"type": "String"
			},
			{
				"name": "title",
				"type": "String"
			},
			{
				"name": "genres",
				"type": "String"
			},
			{
				"name": "year",
				"type": "String"
			},
			{
				"name": "Rating",
				"type": "String"
			},
			{
				"name": "Rotton Tomato",
				"type": "String"
			}
		]
	}
}